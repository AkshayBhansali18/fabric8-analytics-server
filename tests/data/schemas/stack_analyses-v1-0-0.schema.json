{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "definitions": {
        "component_blackduck_info": {
            "type": "object",
            "description": "Blackduck information for a single component",
            "properties": {
                "license": {
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/component_blackduck_license_info"
                    }
                },
                "security": {
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/component_blackduck_security_info"
                    }
                }
            },
            "additionalProperties": false
        },
        "component_blackduck_license_info": {
            "type": "object",
            "description": "Blackduck information about one license for a single component",
            "properties": {
                "codeSharing": {
                    "type": "string"
                },
                "name": {
                    "type": "string"
                }
            },
            "required": [
                "codeSharing",
                "name"
            ],
            "additionalProperties": false
        },
        "component_blackduck_security_info": {
            "type": "object",
            "description": "Blackduck information about one vulnerability for a single component",
            "properties": {
                "baseScore": {
                    "type": "number"
                },
                "exploitabilitySubscore": {
                    "type": "number"
                },
                "id": {
                    "type": "string"
                },
                "severity": {
                    "type": "string"
                },
                "source": {
                    "type": "string"
                }
            },
            "required": [
                "baseScore",
                "exploitabilitySubscore",
                "id",
                "severity",
                "source"
            ],
            "additionalProperties": false
        },
        "component_cve_info": {
            "type": "object",
            "description": "Information about one CVE for a single component",
            "properties": {
                "cvss": {
                    "type": "number"
                },
                "id": {
                    "type": "string"
                }
            },
            "required": [
                "cvss",
                "id"
            ],
            "additionalProperties": false
        },
        "component_github_info": {
            "type": "object",
            "description": "Github information for a single component",
            "properties": {
                "forks_count": {
                    "type": "number"
                },
                "stargazers_count": {
                    "type": "number"
                }
            },
            "required": [
                "forks_count",
                "stargazers_count"
            ],
            "additionalProperties": false
        },
        "component_info": {
            "type": "object",
            "description": "Information about a single component",
            "properties": {
                "blackduck_details": {
                    "$ref": "#/definitions/component_blackduck_info"
                },
                "cve_details": {
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/component_cve_info"
                    }
                },
                "ecosystem": {
                    "type": "string"
                },
                "github_details": {
                    "$ref": "#/definitions/component_github_info"
                },
                "id": {
                    "type": "string"
                },
                "latest_version": {
                    "oneOf": [
                        {
                            "type": "string"
                        },
                        {
                            "type": "null"
                        }
                    ]
                },
                "licenses": {
                    "type": "array",
                    "items": {
                        "type": "string"
                    }
                },
                "name": {
                    "type": "string"
                },
                "package_dependents_count": {
                    "type": "number"
                },
                "version": {
                    "type": "string"
                }
            },
            "required": [
                "blackduck_details",
                "cve_details",
                "ecosystem",
                "github_details",
                "id",
                "latest_version",
                "licenses",
                "name",
                "package_dependents_count",
                "version"
            ],
            "additionalProperties": false
        },
        "stack_analysis_report": {
            "type": "object",
            "description": "Stack analysis report with aggregated data",
            "properties": {
                "analyzed_components": {
                    "type": "number"
                },
                "total_security_issues": {
                    "type": "number"
                },
                "total_licenses": {
                    "type": "number"
                },
                "components_with_security_issues": {
                    "type": "array",
                    "items": {
                        "type": "string"
                    }
                },
                "distinct_licenses": {
                    "type": "array",
                    "items": {
                        "type": "string"
                    }
                },
                "components": {
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/component_info"
                    }
                }
            },
            "required": [
                "analyzed_components",
                "total_security_issues",
                "total_licenses",
                "components_with_security_issues",
                "distinct_licenses",
                "components"
            ],
            "additionalProperties": false
        }
    },
    "type": "object",
    "description": "Stack analysis",
    "properties": {
        "schema": {
            "type": "object",
            "properties": {
                "name": {
                    "type": "string",
                    "description": "Name of the schema",
                    "pattern": "^[a-zA-Z0-9_]+$"
                },
                "version": {
                    "type": "string",
                    "description": "Version of the schema",
                    "pattern": "^[0-9]+-[0-9]+-[0-9]+$"
                },
                "url": {
                    "type": "string",
                    "description": "Full URL of the schema",
                    "format": "uri"
                }
            },
            "required": [
                "name",
                "version"
            ],
            "additionalProperties": false
        },
        "status": {
            "type": "string",
            "enum": [
                "FINISHED",
                "FAILED",
                "INPROGRESS"
            ]
        },
        "submitted_at": {
            "type": "string",
            "format": "date-time"
        },
        "started_at": {
            "type": "string",
            "format": "date-time"
        },
        "finished_at": {
            "type": "string",
            "format": "date-time"
        },
        "request_id": {
            "type": "string"
        },
        "analyses_result": {
            "type": "array",
            "items": {
                "type": "string"
            }
        },
        "result": {
            "type": "object",
            "description": "Stack analysis result",
            "properties": {
                "stack-analyses": {
                    "$ref": "#/definitions/stack_analysis_report"
                }
            },
            "additionalProperties": false
        }
    },
    "required": [
        "status",
        "submitted_at",
        "started_at",
        "finished_at",
        "request_id",
        "analyses_result",
        "result"
    ],
    "additionalProperties": false,
    "title": "stack_analysis-v1_0_0"
}